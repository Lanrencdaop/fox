                  <include>rpc.fox:fox-common</include>
                  <include>rpc.fox:fox-common</include>
                  <include>rpc.fox:fox-common</include>
                </includes>
              </artifactSet>
              <transformers>
                <transformer>
                  <resource>META-INF/spring.handlers</resource>
                </transformer>
                <transformer>
                  <resource>META-INF/spring.schemas</resource>
                </transformer>
                <transformer>
                  <resource>META-INF/spring.tooling</resource>
                </transformer>
                <transformer />
                <transformer />
              </transformers>
            </configuration>
          </execution>
        </executions>
